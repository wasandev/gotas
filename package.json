{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-input-thai-address": "cd nova-components/InputThaiAddress && npm run dev",
        "build-input-thai-address-prod": "cd nova-components/InputThaiAddress && npm run prod",
        "build-tmsdashboard": "cd nova-components/Tmsdashboard && npm run dev",
        "build-tmsdashboard-prod": "cd nova-components/Tmsdashboard && npm run prod",
        "build-gotas-menugroup": "cd nova-components/GotasMenugroup && npm run dev",
        "build-gotas-menugroup-prod": "cd nova-components/GotasMenugroup && npm run prod",
        "build-tranjob_entry": "cd nova-components/TranjobEntry && npm run dev",
        "build-tranjob_entry-prod": "cd nova-components/TranjobEntry && npm run prod",
        "build-tranjob": "cd nova-components/views/tranjob && npm run dev",
        "build-tranjob-prod": "cd nova-components/views/tranjob && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19.0",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.1",
        "jquery": "^3.4.0",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.22.9",
        "sass-loader": "^13.2.0",
        "tailwindcss": "^1.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.2"
    },
    "dependencies": {
        "babel-loader": "^8.3.0",
        "css-loader": "^6.7.3",
        "laravel-echo": "^1.5.3",
        "postcss-import": "^12.0.1",
        "pusher-js": "^4.4.0",
        "vue-loader": "^17.0.1",
        "vue-thai-address-input": "^0.2.1"
    }
}
