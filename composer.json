{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "beyondcode/laravel-websockets": "^1.1",
        "coreproc/nova-notification-feed": "^1.3",
        "dillingham/nova-ajax-select": "^1.0",
        "dniccum/nova-documentation": "v1.0.2.1",
        "epartment/nova-dependency-container": "^1.1",
        "fideloper/proxy": "^4.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.6.1",
        "guzzlehttp/guzzle": "^6.3",
        "hyn/multi-tenant": "5.4.*",
        "invato/redirect-to-index": "dev-master",
        "jobcerto/nova-grid": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/nova": "*",
        "laravel/scout": "^7.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/laravel-nova-excel": "^1.1",
        "pusher/pusher-php-server": "^3.4",
        "ramsey/uuid": "^3.8",
        "stephenlake/nova-fixed-bars": "^1.1",
        "titasgailius/search-relations": "^1.0",
        "wasandev/gotas": "*",
        "wasandev/gotas-menugroup": "*",
        "wasandev/input-thai-address": "*",
        "wasandev/tmsdashboard": "*",
        "wemersonrv/input-mask": "^0.1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.0"
    },
    "repositories": [{
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/InputThaiAddress"
        },
        {
            "type": "path",
            "url": "./nova-components/Tmsdashboard"
        },
        {
            "type": "path",
            "url": "./nova-components/Gotas"
        },
        {
            "type": "path",
            "url": "./nova-components/GotasMenugroup"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/seeds/tenant",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
